pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello from git.'
            }
        }
        
        stage('Build') {
            steps {
                echo 'Building ......'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'Deploying.......'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Testing.........'
            }
        }
    }
}
